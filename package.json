{
  "name": "babysitter",
  "displayName": "Babysitter",
  "description": "VS Code extension for orchestrating and monitoring `o` runs.",
  "version": "0.0.9",
  "publisher": "a5c-ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/a5c-ai/babysitter.git"
  },
  "license": "UNLICENSED",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "a5c",
    "o",
    "orchestration",
    "monitoring",
    "runs"
  ],
  "extensionKind": [
    "workspace"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [
    "onStartupFinished",
    "onView:babysitter.runs",
    "onCommand:babysitter.activate",
    "onCommand:babysitter.dispatchRun",
    "onCommand:babysitter.resumeRun",
    "onCommand:babysitter.sendEsc",
    "onCommand:babysitter.sendEnter",
    "onCommand:babysitter.locateOBinary",
    "onCommand:babysitter.installOInWorkspace",
    "onCommand:babysitter.showConfigurationErrors",
    "onCommand:babysitter.openPromptBuilder",
    "onCommand:babysitter.dispatchRunFromTaskFile",
    "onCommand:babysitter.openRunDetails",
    "onCommand:babysitter.openRunLogs",
    "onCommand:babysitter.revealRunFolder",
    "onCommand:babysitter.archiveRun",
    "onCommand:babysitter.markRunComplete",
    "onCommand:babysitter.runs.refresh"
  ],
  "contributes": {
    "commands": [
      {
        "command": "babysitter.activate",
        "title": "Babysitter: Activate (Log Output)"
      },
      {
        "command": "babysitter.dispatchRun",
        "title": "Babysitter: Dispatch Run"
      },
      {
        "command": "babysitter.dispatchRunFromTaskFile",
        "title": "Babysitter: Dispatch Run from Task File"
      },
      {
        "command": "babysitter.resumeRun",
        "title": "Babysitter: Resume Run"
      },
      {
        "command": "babysitter.sendEsc",
        "title": "Babysitter: Send ESC to `o`"
      },
      {
        "command": "babysitter.sendEnter",
        "title": "Babysitter: Send Enter to `o`"
      },
      {
        "command": "babysitter.locateOBinary",
        "title": "Babysitter: Locate `o` Binary"
      },
      {
        "command": "babysitter.installOInWorkspace",
        "title": "Babysitter: Install/Update `o` in Workspace"
      },
      {
        "command": "babysitter.showConfigurationErrors",
        "title": "Babysitter: Show Configuration Errors"
      },
      {
        "command": "babysitter.openPromptBuilder",
        "title": "Babysitter: Prompt Builder"
      },
      {
        "command": "babysitter.openRunDetails",
        "title": "Babysitter: Open Run Details"
      },
      {
        "command": "babysitter.openRunLogs",
        "title": "Babysitter: Open Run Logs"
      },
      {
        "command": "babysitter.revealRunFolder",
        "title": "Babysitter: Reveal Run Folder in Explorer"
      },
      {
        "command": "babysitter.runs.refresh",
        "title": "Babysitter: Refresh Runs"
      },
      {
        "command": "babysitter.archiveRun",
        "title": "Babysitter: Archive Run"
      },
      {
        "command": "babysitter.markRunComplete",
        "title": "Babysitter: Mark Run Completed"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "babysitter.runs",
          "name": "Babysitter Runs"
        }
      ]
    },
    "menus": {
      "explorer/context": [
        {
          "command": "babysitter.dispatchRunFromTaskFile",
          "when": "resourceScheme == file && resourceFilename =~ /\\.task\\.md$/",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "babysitter.openRunDetails",
          "when": "view == babysitter.runs && viewItem == babysitterRun",
          "group": "navigation"
        },
        {
          "command": "babysitter.openRunLogs",
          "when": "view == babysitter.runs && viewItem == babysitterRun",
          "group": "navigation"
        },
        {
          "command": "babysitter.resumeRun",
          "when": "view == babysitter.runs && viewItem == babysitterRun",
          "group": "navigation"
        },
        {
          "command": "babysitter.revealRunFolder",
          "when": "view == babysitter.runs && viewItem == babysitterRun",
          "group": "navigation"
        },
        {
          "command": "babysitter.markRunComplete",
          "when": "view == babysitter.runs && viewItem == babysitterRun",
          "group": "management"
        },
        {
          "command": "babysitter.archiveRun",
          "when": "view == babysitter.runs && viewItem == babysitterRun",
          "group": "management"
        }
      ],
      "view/title": [
        {
          "command": "babysitter.runs.refresh",
          "when": "view == babysitter.runs",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "babysitter.openPromptBuilder",
        "key": "ctrl+alt+b p",
        "mac": "cmd+alt+b p"
      },
      {
        "command": "babysitter.dispatchRun",
        "key": "ctrl+alt+b n",
        "mac": "cmd+alt+b n"
      },
      {
        "command": "babysitter.resumeRun",
        "key": "ctrl+alt+b r",
        "mac": "cmd+alt+b r"
      },
      {
        "command": "babysitter.openRunDetails",
        "key": "ctrl+alt+b d",
        "mac": "cmd+alt+b d"
      },
      {
        "command": "babysitter.openRunLogs",
        "key": "ctrl+alt+b l",
        "mac": "cmd+alt+b l"
      },
      {
        "command": "babysitter.runs.refresh",
        "key": "ctrl+alt+b shift+r",
        "mac": "cmd+alt+b shift+r"
      },
      {
        "command": "babysitter.sendEnter",
        "key": "ctrl+alt+b enter",
        "mac": "cmd+alt+b enter"
      },
      {
        "command": "babysitter.sendEsc",
        "key": "ctrl+alt+b escape",
        "mac": "cmd+alt+b escape"
      }
    ],
    "configuration": {
      "title": "Babysitter",
      "properties": {
        "babysitter.o.binaryPath": {
          "type": "string",
          "default": "",
          "description": "Optional full path to the `o` executable (or a directory containing it). If empty, Babysitter looks for `o` in the workspace root first, then on PATH."
        },
        "babysitter.oPath": {
          "type": "string",
          "default": "",
          "description": "Legacy alias for `babysitter.o.binaryPath`. Optional full path to the `o` executable (or a directory containing it)."
        },
        "babysitter.runsRoot": {
          "type": "string",
          "default": ".a5c/runs",
          "description": "Runs root directory. Relative paths are resolved from the workspace root."
        },
        "babysitter.globalConfigPath": {
          "type": "string",
          "default": "",
          "description": "Optional path to a JSON global config file. Supported keys: `oBinaryPath`, `runsRoot`."
        },
        "babysitter.o.install.bashPath": {
          "type": "string",
          "default": "",
          "description": "Windows only: optional full path to a Bash executable (e.g. Git Bash `bash.exe`) for running the upstream `o` installer when WSL is unavailable."
        },
        "babysitter.dispatch.shellPath": {
          "type": "string",
          "default": "",
          "description": "macOS/Linux: optional shell executable to use when launching dispatch terminals. Defaults to $SHELL or /bin/bash."
        },
        "babysitter.dispatch.shellArgs": {
          "type": [
            "array",
            "string"
          ],
          "items": {
            "type": "string"
          },
          "default": [
            "-l"
          ],
          "description": "macOS/Linux: additional shell arguments for dispatch terminals. Provide a string or array; use an empty array to disable the -l default."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\"",
    "build": "npm run clean && tsc -p .",
    "watch": "tsc -watch -p .",
    "lint": "eslint . --ext .ts",
    "verify:metadata": "node ./scripts/check-package-metadata.cjs",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "package": "vsce package --allow-missing-repository",
    "test:unit": "npm run build && npm run test:unit:compiled",
    "test:unit:compiled": "mocha --ui tdd \"dist/unit/**/*.test.js\"",
    "test:extension": "npm run build && npm run test:extension:compiled",
    "test:extension:compiled": "node ./dist/test/runTest.js",
    "test": "npm run build && npm run test:unit:compiled && npm run test:extension:compiled",
    "test:ci": "npm run build && npm run test:unit:compiled && node ./dist/test/runTest.js --headless",
    "test:sdk": "npm run test --workspace=@a5c/babysitter-sdk",
    "build:sdk": "npm run build --workspace=@a5c/babysitter-sdk"
  },
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.30",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vscode/test-electron": "^2.4.0",
    "@vscode/vsce": "^3.7.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "mocha": "^10.4.0",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "dompurify": "^3.3.1",
    "jsdom": "^27.4.0",
    "mermaid": "^10.9.1",
    "node-pty": "^1.1.0",
    "typescript": "^5.5.4"
  }
}
